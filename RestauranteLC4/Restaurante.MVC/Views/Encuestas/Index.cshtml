@model IEnumerable<Restaurante.Entities.Encuesta>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Resultado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SucursalId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Resultado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SucursalId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EncuestaId }) |
            @Html.ActionLink("Details", "Details", new { id=item.EncuestaId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EncuestaId })
        </td>
    </tr>
}

</table>
