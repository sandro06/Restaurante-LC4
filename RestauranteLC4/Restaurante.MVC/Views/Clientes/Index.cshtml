@model IEnumerable<Restaurante.Entities.Cliente>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Pedido.PedidoId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Reserva.Referencia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApeMat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ApePat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dni)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Direccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReservaId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Pedido.PedidoId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Reserva.Referencia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ApeMat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ApePat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dni)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReservaId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ClienteId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ClienteId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ClienteId })
        </td>
    </tr>
}

</table>
